<div class="row navbar navbar-expand-lg navbar-light bg-light">
  <div class="col-md-4">
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
    	<h3>Admin</h3>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item active">
  
          </li>
        </ul>
      </div>
    </nav>
  </div>
  <div class="col-md-6">
  
  </div>
  <div class="col-md-2 ">
    <div class="row">
    	<div class="col-md-6">
	      <% if administrator_signed_in? %>
	        <%= form_tag(destroy_administrator_session_path, method: :delete) do %>
	          <%= csrf_meta_tags %>
	          <%= submit_tag 'Logout', class: 'btn btn-danger btn-sm' %>
	        <% end %>
	      <% end %>
    	</div>
    	<div class="col-md-6">
    		<% if administrator_signed_in? %>
          <%= link_to "My Account", administrators_path, class: "btn btn-primary btn-sm" %>
        <% else %>
          <%= link_to "Sign In", new_administrator_session_path, class: "btn btn-primary btn-sm float-right" %>
        <% end %>
    	</div>
    </div>
  </div>
</div>
